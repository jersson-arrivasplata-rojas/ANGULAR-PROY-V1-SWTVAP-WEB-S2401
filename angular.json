{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "swtvap",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-proy-v1-swtvap-web-administrator-s2401",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "./src/favicon.ico",
              "./src/assets"
            ],
            "styles": [
              "./node_modules/bootstrap/dist/css/bootstrap.css",
              "./node_modules/animate.css/animate.min.css",
              "./src/assets/css/hopscotch.css",
              "./src/assets/css/getemoji.css",
              "./src/assets/css/app.css",
              "./src/assets/css/icons.min.css",
              "./src/assets/css/iconsmind.css",
              "./src/assets/css/vendor/quill.bubble.css",
              "./src/assets/css/vendor/quill.snow.css",
              "./src/styles.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./src/assets/js/vendor/hopscotch.min.js",
              "./node_modules/webfontloader/webfontloader.js"
            ]
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            },
            "cert": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.cert.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            }
          },
          "defaultConfiguration": "prod"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "prod": {
              "browserTarget": "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401:build:prod"
            },
            "local": {
              "browserTarget": "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401:build:local",
              "host": "0.0.0.0"
            },
            "dev": {
              "browserTarget": "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401:build:dev"
            },
            "cert": {
              "browserTarget": "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401:build:cert"
            }
          },
          "defaultConfiguration": "local",
          "options": {
            "browserTarget": "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401:build",
            "port": 4200
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ANGULAR-PROY-V1-SWTVAP-WEB-ADMINISTRATOR-S2401:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
